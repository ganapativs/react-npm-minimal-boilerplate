{
  "name": "react-npm-minimal-boilerplate",
  "version": "2.0.0",
  "description": "Minimal boilerplate for creating React NPM packages",
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/ganapativs/react-npm-boilerplate.git"
  },
  "author": "Ganapati V S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganapativs/react-npm-boilerplate/issues"
  },
  "homepage": "https://github.com/ganapativs/react-npm-boilerplate",
  "keywords": [
    "react-boileplate",
    "react-component",
    "boileplate",
    "component",
    "react"
  ],
  "scripts": {
    "prepublish": "babel --plugins '@babel/transform-modules-umd' src --out-dir ./dist",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.40",
    "@babel/plugin-transform-modules-umd": "7.0.0-beta.40",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.40",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-react": "7.0.0-beta.40",
    "@babel/preset-stage-0": "7.0.0-beta.40",
    "@babel/runtime": "7.0.0-beta.40",
    "babel-eslint": "^8.2.2",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-minify-guarded-expressions": "^0.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1"
  },
  "peerDependencies": {
    "react": "^16.3.0-alpha.2",
    "react-dom": "^16.3.0-alpha.2"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.0-alpha.2",
    "react-dom": "^16.3.0-alpha.2"
  }
}
